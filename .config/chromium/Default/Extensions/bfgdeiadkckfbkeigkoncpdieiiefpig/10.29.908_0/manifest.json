{
   "background": {
      "persistent": true,
      "scripts": [ "dist/chrome-extension-background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/bitmoji-icon-19.png",
         "38": "images/bitmoji-icon-38.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "thirdparty/webpicker-sdk.css", "dist/chrome-extension.css" ],
      "js": [ "thirdparty/inbox-sdk.js", "thirdparty/jquery.min.js", "thirdparty/sweetalert.min.js", "thirdparty/webpicker-sdk.js", "dist/bitmoji-chrome-extension.js", "dist/chrome-extension-page-detector.js" ],
      "matches": [ "https://mail.google.com/*", "https://inbox.google.com/*", "https://bitbucket.org/*", "https://*.github.com/*", "https://*.atlassian.net/*", "*://*.bitmoji.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Use your personal emoji in your favorite sites!",
   "externally_connectable": {
      "matches": [ "https://test.bitmoji.com/account_v2/*", "https://www.bitmoji.com/account_v2/*" ]
   },
   "icons": {
      "128": "images/bitmoji-logo-128.png",
      "48": "images/bitmoji-logo-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Bitmoji",
   "permissions": [ "tabs", "storage", "cookies", "clipboardWrite", "notifications", "webRequest", "webRequestBlocking", "webNavigation", "*://mail.google.com/*", "*://inbox.google.com/*", "*://*.bitbucket.org/*", "*://*.github.com/*", "*://*.atlassian.net/*", "*://*.bitmoji.com/*" ],
   "short_name": "Bitmoji",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "10.29.908",
   "web_accessible_resources": [ "static/media/*.svg", "static/media/*.png" ]
}
